package layouts

import (
  "github.com/byvko-dev/youtube-app/internal/templates/layouts/partials"
)

templ HeadOnly(content ...templ.Component) {
	<html lang="en" class="h-full">
		@partials.Head()
		<body class="h-full" id="main">
			for _, c := range content {
				@c
			}
			{ children... }
		</body>
	</html>
}

