package layouts

import (
  "github.com/cufee/feedlr-yt/internal/templates/layouts/partials"
)

templ HeadOnly(content ...templ.Component) {
	<html lang="en" class="h-full">
		@partials.Head()
		<body class="h-full">
			<div id="main" class="w-full h-full">
				for _, c := range content {
					@c
				}
				{ children... }
			</div>
		</body>
	</html>
}

